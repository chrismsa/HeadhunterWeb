<h1><%= Profile.model_name.human %></h1>
<%= form_with model: @profile, local: :true  do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :social_name %>
  <%= f.text_field :social_name %>
  <%= f.label :date_of_birth %>
  <%= f.date_field :date_of_birth %>
  <%= f.label :formations %>
  <%= f.text_field :formations %>
  <%= f.label :xp %>
  <%= f.text_field :xp %>
  <%= f.submit Profile.human_attribute_name('save') %>
<% end %>